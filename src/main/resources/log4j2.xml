<?xml version="1.0" encoding="UTF-8"?>
<Configuration monitorInterval="30">
  <Appenders>
    <Console name="Console">
      <PatternLayout pattern="d{YYYY-MM-dd HH:mm:ss.SSS} [%t] [%logger{36}:%M] [%-5level] %msg%n"/>
    </Console>
  </Appenders>
  <Loggers>
    <Root level="off">
<!--       <AppenderRef ref="Console"/> -->
    </Root>
  </Loggers>
</Configuration>
